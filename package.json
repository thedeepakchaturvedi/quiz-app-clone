{
    "name": "quiz-app",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "clean": "turbo run clean && rm -rf .turbo && rm -rf node_modules && rm -rf data",
        "format": "prettier --write \"**/*.{ts,tsx,md,js,jsx,css,json,yml,yaml}\""
    },
    "devDependencies": {
        "eslint-config-custom": "*",
        "prettier": "latest",
        "turbo": "^1.8.2"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "packageManager": "npm@9.4.0",
    "dependencies": {
        "react-icons": "^4.7.1"
    }
}
